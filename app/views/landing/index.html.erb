<h1>Landing Page</h1>

<% if current_user %>
  <h3>Existing Users</h3>
  <% @users.each do |user| %>
    <div id="user-<%= user.id %>">
      <ul>
        <li><%= user.email %></li>
      </ul>
    </div>
  <% end %>
  <%= button_to "Log Out", logout_path, method: :delete %>
<% else %>
  <%= button_to "Log In", login_path, method: :get %>
  <%= button_to "New User", register_path, method: :get %>
<% end %>



